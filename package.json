{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:fast": "vite --host --force",
    "build": "vite build",
    "build:vercel": "vite build --mode production",
    "build:optimized": "node build-optimized.js",
    "build:vercel-optimized": "node build-vercel.js",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --host",
    "start": "npm run dev",
    "clean": "rm -rf node_modules/.vite dist .vite",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod --config vercel-production.json",
    "deploy:domain": "vercel --prod --config vercel-domain-config.json",
    "deploy:optimized": "node deploy-vercel.js && vercel --prod",
    "vercel:build": "npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "lucide-react": "^0.543.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.0",
    "recharts": "^3.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.5"
  }
}
